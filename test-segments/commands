clang -S hello_pierre.c -mllvm --generate-segment-scheme -mllvm -debug
clang -c hello_pierre.c -mllvm --generate-segment-scheme -mllvm --segment-frame-pointer -mllvm --debug
clang -c hello_pierre.c -mllvm --generate-segment-scheme -mllvm --segment-frame-pointer -mllvm --generate-segment-globals -fpic
